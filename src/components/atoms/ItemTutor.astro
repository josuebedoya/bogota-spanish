---
interface Props {
  image: string;
  name: string;
  chargue: string;
}
const { image, name, chargue } = Astro.props;
---

<div class="single-team-item style-01">
  <div class="thumbnail">
    <img src={image} alt={name} />
    <ul class="ul social-media-list style-01">
      <li class="single-social-item">
        <a href="#">
          <i class="fa-solid fa-eye icon"></i>
        </a>
      </li>
    </ul>
  </div>
  <div class="content">
    <h4 class="title">
      <a href="#">{name}</a>
    </h4>
    <p class="designation">{chargue}</p>
  </div>
</div>
